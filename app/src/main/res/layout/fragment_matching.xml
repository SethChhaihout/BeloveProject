<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.planbcambodia.beloveproject.fragment.MatchingFragment">

    <include
        android:id="@+id/main_page_toolbar"
        layout="@layout/main_toolbar" />

    <link.fls.swipestack.util.MySwipeStack
        android:id="@+id/swipeStack"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="10dp"
        app:stack_rotation="0"
        app:stack_size="2"
        app:stack_spacing="0dp" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/ic_love"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:scaleType="center"
            android:src="@drawable/ic_love" />

        <ImageView
            android:id="@+id/ic_sorry"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_centerVertical="true"
            android:scaleType="center"
            android:src="@drawable/ic_sorry" />
    </RelativeLayout>
</FrameLayout>
